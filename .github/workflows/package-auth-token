name: package-auth-token
on: 
  workflow_call:
    outputs:
      value: 
        description: "Create token"
        value: ${{ jobs.create.outputs.package-auth-token }}

jobs:
  create:
    runs-on: ubuntu-latest
    outputs:
      package-auth-token: ${{ secrets.GITHUB_TOKEN }}

    #steps:
      #- uses: actions/checkout@v4
      - name: Create Package-Auth-Token
        run: echo Create Package-Auth-Token
        #id: Create-package-auth-token


            
    
